{
  "name": "mocha-tcov-reporter",
  "version": "0.0.4",
  "keywords": [
    "mocha",
    "code coverage",
    "reporter",
    "text",
    "file list",
    "simple",
    "blanket"
  ],
  "description": "code coverage reporter for mocha",
  "main": "lib/reporter.js",
  "scripts": {
    "test": "multi='spec=- mocha-lcov-reporter=lcov-report.lcov' ./node_modules/mocha/bin/mocha test",
    "build": "./node_modules/gulp/bin/gulp.js --require coffee-script coffee",
    "coveralls": "./node_modules/gulp/bin/gulp.js --require coffee-script coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/holyshared/mocha-tcov-reporter.git"
  },
  "author": "Noritaka Horio <holy.shared.design@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/holyshared/mocha-tcov-reporter/issues"
  },
  "homepage": "https://github.com/holyshared/mocha-tcov-reporter",
  "dependencies": {
    "lodash": "~3.9.0",
    "mocha": "~2.2.0",
    "sprintf-js": "~1.0.2"
  },
  "devDependencies": {
    "blanket": "~1.1.7",
    "chai": "~3.0.0",
    "coffee-script": "~1.8.0",
    "coveralls": "~2.11.2",
    "gulp": "~3.8.10",
    "gulp-coffee": "~2.2.0",
    "gulp-run": "~1.6.6",
    "gulp-sourcemaps": "~1.3.0",
    "gulp-util": "~3.0.2",
    "mocha-lcov-reporter": "~0.0.2",
    "mocha-multi": "~0.7.1",
    "sinon": "~1.12.2"
  },
  "config": {
    "blanket": {
      "pattern": "/lib/",
      "data-cover-never": "/node_modules/"
    }
  }
}
